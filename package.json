{
  "name": "vuepress-blog",
  "version": "0.0.1",
  "description": "Finen's Blog",
  "main": "lib/index.js",
  "bin": {
    "vuepress": "bin/vuepress.js"
  },
  "scripts": {
    "dev": "node bin/vuepress dev docs",
    "build": "node bin/vuepress build docs",
    "docs:build": "npm run docs:build",
    "lint": "eslint --fix --ext .js,.vue bin/ lib/ test/",
    "deploy-gh": "yarn build && bash scripts/deploy-gh.sh",
    "prepublishOnly": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "release": "/bin/bash scripts/release.sh",
    "test": "node test/prepare.js && jest --config test/jest.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirCodd/vuepress-blog.git"
  },
  "keywords": [
    "documentation",
    "vue",
    "generator"
  ],
  "author": "hefengen",
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
